package garbagecollection;

public class EmployeeMain 
{
	public static void main(String[] args) {
		Employee e1=new Employee("gayathri",32);
		Employee e2=new Employee("Ravi",22);
		Employee e3=new Employee("ravina",42);
		e1.show();
		e2.show();
		e3.show();
		e1.show1();
		e2.show1();
		e3.show1();
		Employee e4=new Employee("thara",22);
		Employee e5=new Employee("pravin",42);
		e4.show();
		e4.show1();
		e5.show();
		e5.show1();
		
		e3.show1();
				
	}

}
